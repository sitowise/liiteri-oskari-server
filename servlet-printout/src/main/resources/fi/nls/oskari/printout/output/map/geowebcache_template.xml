							<double>0</double>
							<double>6500000</double>
							<double>750000</double>
							<double>8000000</double>
						</coords>
					</extent>
					<zoomStart>0</zoomStart>
				</gridSubset>
			</gridSubsets>
			<useETags>true</useETags>
			<wmsUrl>
				<string>http://127.0.0.1:80/geoserver/service/wms</string>
			</wmsUrl>
			<wmsLayers>LAYER_TEMPLATE</wmsLayers>
